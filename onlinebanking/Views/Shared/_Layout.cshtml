<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Styles/HomeStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/LoginStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/AccountStyle.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Styles/CustomerStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/StatementStyle.css")" rel="stylesheet" type="text/css" />
   


    <style>
        
    </style>

</head>
<body>
   
        <div >
           
            <div class="container-body">
                @RenderBody()
                <hr />
                <footer>
                    <p>&copy; @DateTime.Now.Year - online banking</p>
                </footer>
            </div>
        </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
