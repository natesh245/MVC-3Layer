<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link href="@Url.Content("~/Styles/HomeStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/LoginStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/AccountStyle.css")" rel="stylesheet" type="text/css" />

    <link href="@Url.Content("~/Styles/CustomerStyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/StatementStyles.css")" rel="stylesheet" type="text/css" />
    <script src="https://unpkg.com/ionicons@5.0.0/dist/ionicons.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css" integrity="sha256-2XFplPlrFClt0bIdPgpz8H7ojnk10H69xRqd9+uTShA=" crossorigin="anonymous" />
   <style>
       body{
           overflow:scroll;
       }
   </style>
    

</head>
<body>

    <div>

        <div class="container-body">
            @RenderBody()
           
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @RenderSection("scripts2", required: false)

</body>
</html>
