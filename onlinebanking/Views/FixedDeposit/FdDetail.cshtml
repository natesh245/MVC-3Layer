@model BussinessLayer.Models.FdDetailsModel

@{
    ViewBag.Title = "FdDetail";
}



    <div class="layout-container">
        <div>
            @{
                Html.RenderPartial("_SideNavBar");
            }
        </div>
        <div>
            <h2>Fd Details</h2>

           
            <div class="edit-container" >

                @foreach (var item in Model.FdDetailList)
                {
                    <div class="account-container">

                        <dl>
                            <dt>
                                @Html.DisplayNameFor(model => item.AccountNo)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.AccountNo)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.DepositId)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.DepositId)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.FdDate)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.FdDate)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.FdAmount)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.FdAmount)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.Duration)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.Duration)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.RateOfInterest)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.RateOfInterest)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.MaturityDate)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.MaturityDate)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.MaturityAmount)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.MaturityAmount)
                            </dd>

                            <dt>
                                @Html.DisplayNameFor(model => item.Nominee)
                            </dt>

                            <dd>
                                @Html.DisplayFor(model => item.Nominee)
                            </dd>

                        </dl>
                    </div>

                }
            </div>

        </div>
</div>
