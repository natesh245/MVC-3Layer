@model onlinebanking.Models.FdDetails

@{
    ViewBag.Title = "FdDetail";
}



    <div class="layout-container">
        <div>
            @{
                Html.RenderPartial("_SideNavBar");
            }
        </div>
        <div>
            <h4>Fd Details</h4>

            <hr />
            <p>

                @Html.ActionLink("Back ", "Details", "Account", new { id = @Session["customerid"] }, new { @class = "btn btn-primary" })
            </p>
            @foreach (var item in Model.FdDetailList)
            {
                <div class="account-container">

                    <dl>
                        <dt>
                            @Html.DisplayNameFor(model => item.account_no)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.account_no)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.deposit_id)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.deposit_id)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.fd_date)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.fd_date)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.fd_amount)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.fd_amount)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.duration)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.duration)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.rate_of_interest)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.rate_of_interest)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.maturity_date)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.maturity_date)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.maturity_amount)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.maturity_amount)
                        </dd>

                        <dt>
                            @Html.DisplayNameFor(model => item.nominee)
                        </dt>

                        <dd>
                            @Html.DisplayFor(model => item.nominee)
                        </dd>

                    </dl>
                </div>

            }

        </div>
</div>
